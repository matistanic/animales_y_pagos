<p id="notice"><%= notice %></p>

<h1>Transbanks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transbanks.each do |transbank| %>
      <tr>
        <td><%= link_to 'Show', transbank %></td>
        <td><%= link_to 'Edit', edit_transbank_path(transbank) %></td>
        <td><%= link_to 'Destroy', transbank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transbank', new_transbank_path %>
